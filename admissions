<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
<script src="https://parsleyjs.org/dist/parsley.min.js"></script>
<script src="../media/mod_jdsimplecontactform/assets/js/moment.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/pikaday/pikaday.js"></script>
<script>
      var jdscf_picker_173 = new Pikaday({field: document.getElementById("jdscf-173-dob"),format: "MM-DD-YYYY",defaultDate: moment("2020-07-29").toDate(),setDefaultDate:true});   </script>
<script> var jQuery_3_3_1 = $.noConflict(true);</script>
<script>
         (function ($) {
            $(function () {
               var showMessage173 = function (type, message) {
                  type = type == 'error' ? 'danger' : type;
                  var _alert = '<div class="alert alert-' + type + '"><div>' + message + '</div></div>';
                  $('#jdscf-message-173').html(_alert);
                  setTimeout(function () {
                     $('#jdscf-message-173').html('');
                  }, 3000);
               }
               $('#simple-contact-form-173').on('submit', function (e) {
                  e.preventDefault();
                  var formData = new FormData(this);
                  var _form = $(this);
                  var _id = 'simple-contact-form-173';
                  var _loading = $('.simple-contact-form-loader.module-173');
                  if (_form.parsley().isValid()) {
                     $.ajax({
                        url: 'https://demo.joomdev.com/gurukul/toolbar/index.php?option=com_ajax&module=jdsimplecontactform&format=json&method=submitForm',
                        data: formData,
                        type: 'POST',
                        beforeSend: function () {
                           _loading.removeClass('d-none');
                        },
                        cache: false,
                        contentType: false,
                        processData: false,
                        success: function (response) {
                           if (response.status == 'success') {
                              $('.jd-simple-contact-message-173').html(response.data.message);
                              _loading.addClass('d-none');
                              if (response.data.redirect != '') {
                                 setTimeout(function () {
                                    window.location = response.data.redirect;
                                 }, 2000);
                              }
                           } else {
                              var errors = JSON.parse(response.message);
                              _loading.addClass('d-none');                              

                              for (index = 0; index < errors.length; ++index) {
                                 showMessage173("error", errors[index]);
                              }

                           }
                        },
                        error: function (response) {
                           _loading.addClass('d-none');
                           showMessage173("error", "Something went wrong! Please try again.");
                        }
                     });
                  }
               });
            });
         })(jQuery_3_3_1);
      </script> </div>
